<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.esquared.tictactoeextreme">

    <application
        android:allowBackup="true"
        android:icon="@drawable/hashtag"
        android:label="@string/app_name"
        android:roundIcon="@drawable/hashtag"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".play_devils"
            android:screenOrientation="portrait" ></activity>
        <activity android:name=".PlayRandomTicTacToe" />
        <activity android:name=".PlayNumberScrabble" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
